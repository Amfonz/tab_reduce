{
    "description" : "reduce open tabs into a list of urls to conserve memory/boost performance",
    "manifest_version" : 2,
    "name" : "tab_reduce",
    "version" : "1.0",
    "permissions" : [
        "tabs"
    ],
    "browser_action" : {
        "browser_style" : true,
        "default_icon" : {
            "48" : "icons/toolbar_icon_mod.png"
        }
    },
    "background" : {
        "scripts" : ["background.js"]
    }
    
    
    
}





